Algoritmo "Exe1"

Var
  nome : caractere
  sobrenome : caractere
  resultado : caractere

Inicio
  escreva("Escreva seu primeiro nome: ")
  leia(nome)
  
  escreva("Escreva seu sobrenome: ")
  leia(sobrenome)
  
  resultado <- nome+" "+sobrenome
  
  escreva("Seu nome completo é: ", resultado)


Fimalgoritmo