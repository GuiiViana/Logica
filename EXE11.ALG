Algoritmo "Exe11"

Var
  nota1 : real
  nota2 : real
  nota3 : real
  media : real

Inicio
  escreva("Digite sua primeira nota: ")
  leia(nota1)

  escreva("Digite sua segunda nota: ")
  leia(nota2)
  
  escreva("Digite sua terceira nota: ")
  leia(nota3)
  
  media <- (nota1*2 + nota2*3 + nota3*5) / 10
  
  escreva("sua media é: ", media)

Fimalgoritmo