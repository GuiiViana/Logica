Algoritmo "EXE2-VisualG"

Var
  temperatura : real


Inicio
   escreva("Digite a temperatura do ambiente em graus: ")
   leia(temperatura)
   
   se(temperatura >= 25) entao
                  escreva("O ambiente está muito quente!!!")
   senao(temperatura < 25) entao
                  escreva("O ambiente está agradevel!!!")
   fimse


Fimalgoritmo